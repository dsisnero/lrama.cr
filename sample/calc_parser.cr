class CalcParser
  def self.run(io : IO = STDIN)
    input = io.gets_to_end
    parser = new
    parser.parse(CalcParserLexer.new(input))
    parser
  end
end




class CalcParser
  def results
    @results ||= [] of Int32
  end
end

